<style type="text/css">
  .root {
    display: flex;
    width: 100%;
    height: 100%;
    flex-direction: column;
    {% if hero_image %}
    background-image: url('{{ hero_image | imageDataUrl }}');
    background-repeat: no-repeat;
    background-size: cover;
    background-position: center center;
    {% endif %}
  }

  .container {
    background-color: {{ accent | default('black') }};
    opacity: {% if hero_image %}0.5{% else %}1{% endif %};
    display: flex;
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    flex-direction: column;
  }

  .bar {
    width: 100%;
    height: 8em;
    color: #262626;
    background-color: #161618;
    background-image: url('data:image/png;base64,');
    background-repeat: no-repeat;
    background-position: 40px 40px;
    background-size: 220px 44px;
  }

  .bottom {
    display: flex;
    flex-direction: column;
    color: white;
    padding: 3em;
    margin-top: auto;
  }

  h1 {
    font-size: 45px;
    margin-bottom: 15px;
  }

  h2 {
    font-size: 30px;
    margin: 0;
  }

</style>

<div class="root">
  <div class="container">
  </div>
  <div class="bar">-</div>
  {% if not hero_image %}
  <div class="bottom">
    <h1>{{ title | safe }}</h1>
    <h2>{{ description | safe }}</h2>
  </div>
  {% endif %}
</div>
